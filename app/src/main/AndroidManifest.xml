<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.t.realfitness">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.t.realfitness.AllHistoryActivity" />
        <activity android:name="com.t.realfitness.HistoryActivity" />
        <activity android:name="com.t.realfitness.CounterActivity" />
        <activity android:name="com.t.realfitness.ExcerciseActivity" />
        <activity android:name="com.t.realfitness.SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>